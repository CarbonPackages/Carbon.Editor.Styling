    "react-draggable": "^4.4.6",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@neos-project/neos-ui-extensibility": "~8.3.11",
    "@stylexjs/esbuild-plugin": "^0.9.3",
    "esbuild": "^0.24.0",
    "fuzzysearch": "^1.0.3",
    "prettier": "^3.4.2"
  },
  "peerDependencies": {
    "@neos-project/neos-ui-extensibility": "^8.3.0 || ^9.0.0"
